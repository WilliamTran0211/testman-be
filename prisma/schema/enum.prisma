model User {
    id     Int    @id @default(autoincrement())
    status Status @default(ACTIVATE)
}

model Role {
    id     Int    @id @default(autoincrement())
    status Status @default(ACTIVATE)
}

model Permission {
    id     Int    @id @default(autoincrement())
    status Status @default(ACTIVATE)
}

model Project {
    id     Int    @id @default(autoincrement())
    status Status @default(ACTIVATE)
}

model ProjectMember {
    id     Int    @id @default(autoincrement())
    status Status @default(ACTIVATE)
}

model TestCase {
    id     Int    @id @default(autoincrement())
    status Status @default(ACTIVATE)
}

model TestStep {
    id     Int    @id @default(autoincrement())
    status Status @default(ACTIVATE)
}

model TestCaseStep {
    id     Int    @id @default(autoincrement())
    status Status @default(ACTIVATE)
}

model TestData {
    id     Int    @id @default(autoincrement())
    status Status @default(ACTIVATE)
}

model TestCaseData {
    id     Int    @id @default(autoincrement())
    status Status @default(ACTIVATE)
}

enum Status {
    ACTIVATE
    DEACTIVATE
}
