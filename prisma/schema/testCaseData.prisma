model TestData {
    id           Int            @id
    TestCaseData TestCaseData[]

    @@map("test_data")
}

model TestCase {
    id           Int            @id
    TestCaseData TestCaseData[]

    @@map("test_cases")
}

model TestCaseData {
    testCaseId Int
    dataId     Int
    testCase   TestCase @relation(fields: [testCaseId], references: [id])
    data       TestData @relation(fields: [dataId], references: [id])
    assignedAt DateTime @default(now())

    @@id([testCaseId, dataId])
    @@map("test_case_datas")
}
